{% extends 'PROCERGSVPRCoreBundle::base.html.twig' %}

{% block body %}
      <h2 id="try-header">
        Votação de Prioridades do RS
        <br>
      </h2>
      <div class="row">
        <div class="col-md-6">
          <h3>
            Acessar com Título de Eleitor do RS
            <br>
          </h3>
          <form>
            <div class="form-group">
              <label>
                Título de Eleitor
              </label>
              <input class="form-control" type="text">
              <div class="form-group">
                <label>
                  Primeiro Nome
                </label>
                <input class="form-control" type="text">
              </div>
            </div>
          </form>
          <button type="submit" class="btn btn-primary">
            Click!
          </button>
        </div>
        <div class="col-md-6">
          <h3>
            Acessar com Login Cidadão
            <br>
          </h3>
          <div class="well">
            <h3 class="text-center">
    {% for owner in hwi_oauth_resource_owners() %}
    <a href="{{ hwi_oauth_login_url(owner) }}">Isso será um botão</a> <br />
    {% endfor %}              
              <br>
            </h3>
          
            
            <p class="text-right">
              <img src="https://www.facebook.com/images/fb_icon_325x325.png" height="50">
              <img src="http://wp.streetwise.co/wp-content/uploads/2012/06/Larry.png" height="50">
            </p>
          </div>
        </div>
      </div>
{% endblock %}
